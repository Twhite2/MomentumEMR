{
  "name": "@momentum/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "cd ../../packages/database && pnpm generate",
    "build": "next build",
    "postbuild": "cd ../../packages/database && pnpm prisma db push --accept-data-loss --skip-generate || true",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --incremental false",
    "clean": "rimraf .next tsconfig.tsbuildinfo node_modules/.cache",
    "build:clean": "npm run clean && npm run build",
    "verify": "powershell -ExecutionPolicy Bypass -File ./check-build.ps1",
    "precommit": "npm run type-check:strict"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@aws-sdk/s3-request-presigner": "^3.899.0",
    "@hookform/resolvers": "^3.9.1",
    "@momentum/database": "workspace:*",
    "@momentum/ui": "workspace:*",
    "@tanstack/react-query": "^5.62.7",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.469.0",
    "multer": "^2.0.2",
    "next": "^15.1.4",
    "next-auth": "^5.0.0-beta.25",
    "notificationapi-node-server-sdk": "^2.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.7.1",
    "uuid": "^13.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/uuid": "^11.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.4",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
